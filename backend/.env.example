# Mobile Money (MoMo) MTN Integration Settings
# Get these credentials from https://momodeveloper.mtn.com/
MOMO_API_USER=your_api_user_id
MOMO_API_KEY=your_api_key
MOMO_SUBSCRIPTION_KEY=your_subscription_key

# Disbursement-specific credentials (for payroll payouts)
# These may be different from collection credentials
MOMO_DISBURSEMENT_API_USER=your_disbursement_api_user_id
MOMO_DISBURSEMENT_API_KEY=your_disbursement_api_key
MOMO_DISBURSEMENT_SUBSCRIPTION_KEY=your_disbursement_subscription_key

# Environment: 'sandbox' for testing, 'production' for live
MOMO_ENVIRONMENT=sandbox

# Callback URL for payment notifications (webhook)
MOMO_CALLBACK_URL=https://your-domain.com/api/payments/momo/webhook

# Provider identifier
MOMO_PROVIDER=mtn_momo

# Other settings
DATABASE_URL=postgresql://postgres:password@localhost/all_inone
JWT_SECRET_KEY=change-this
SECRET_KEY=change-this
